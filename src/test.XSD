<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
<xsd:element name="body" type="bodyType" />
<xsd:complexType name="bodyType">
	<xsd:sequence>
		<xsd:element name="head" type="headType" />
		<xsd:element name="datas" type="datasType" />
		<xsd:element name="remark" type="xsd:string" />
	</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="headType">
	<xsd:attribute name="versionnum" type="xsd:string"	use="required" />
	<xsd:attribute name="wjscsj" type="xsd:string" use="required" />
	<xsd:attribute name="businessitem" type="xsd:string" use="required" />
	<xsd:attribute name="businesstype" type="xsd:string"	use="required" />
	<xsd:attribute name="linenumber" type="xsd:string"	use="required" />
	<xsd:attribute name="sender" type="xsd:string" use="required" />
</xsd:complexType>
<xsd:complexType name="datasType">
	<xsd:sequence maxOccurs="unbounded">
		<xsd:element name="data" type="dataType" />
	</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="dataType">
	<xsd:sequence minOccurs="1" maxOccurs="1">
		<xsd:element name="item1" type="item1Type" />
		<xsd:element name="item2" type="item2Type" />
